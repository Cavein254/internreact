// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Gender {
  MALE
  FEMALE
  OTHER
}
model User {
  id      String      @id @default(cuid())
  fName String
  lName String
  email String @unique
  password String
  gender Gender @default(FEMALE)
  phone String
  dob String
}