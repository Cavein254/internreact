// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Gender {
  MALE
  FEMALE
  OTHER
}
model Profile {
  id 
  fName String
  lName String
  email String
  password String
  gender Gender @default(Gender.FEMALE)
  phone String
  dob String
}